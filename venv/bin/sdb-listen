#!/home/rodrigo-lopes/projetos/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'sdb==1.8','console_scripts','sdb-listen'
__requires__ = 'sdb==1.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sdb==1.8', 'console_scripts', 'sdb-listen')()
    )
